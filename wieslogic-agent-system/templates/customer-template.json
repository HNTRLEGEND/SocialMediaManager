    "workflow_configuration": {
      "webhook_endpoints": {
        "lead_entry": "/webhooks/{customer_id}/lead",
        "technical_handover": "/webhooks/{customer_id}/technical",
        "sales_handover": "/webhooks/{customer_id}/sales",
        "service_request": "/webhooks/{customer_id}/service"
      },
      "authentication": {
        "type": "bearer_token",
        "token": "GENERATE_UNIQUE_TOKEN_HERE",
        "rotate_every_days": 90
      },
      "rate_limiting": {
        "requests_per_minute": 100,
        "burst_allowance": 20
      }
    },

    "ai_configuration": {
      "openai": {
        "model": "gpt-4-turbo",
        "temperature": 0.2,
        "max_tokens": 2000,
        "api_key_ref": "OPENAI_API_KEY_{CUSTOMER_ID}"
