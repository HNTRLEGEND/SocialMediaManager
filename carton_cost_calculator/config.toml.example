# Streamlit Konfiguration (Beispiel)
# Kopieren Sie diese Datei nach .streamlit/config.toml und passen Sie sie an

[theme]
primaryColor = "#FF6B6B"
backgroundColor = "#FFFFFF"
secondaryBackgroundColor = "#F0F2F6"
textColor = "#262730"
font = "sans serif"

[server]
enableCORS = false
enableXsrfProtection = true
maxUploadSize = 5
headless = true

[browser]
gatherUsageStats = false
serverAddress = "localhost"
serverPort = 8501
